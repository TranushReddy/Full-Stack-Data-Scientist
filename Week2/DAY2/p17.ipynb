   "source": [
    "num = int(input(\"Enter a number: \"))\n",
    "\n",
    "rev = 0\n",
    "n = num\n",
    "while n > 0:\n",
    "    digit = n % 10\n",
    "    rev = rev * 10 + digit\n",
    "    n //= 10\n",
    "\n",
    "print(\"Reversed Number:\", rev)"
   ]
  }
