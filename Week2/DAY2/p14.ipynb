   "source": [
    "text = input(\"Enter a string: \")\n",
    "\n",
    "char_count = {}\n",
    "\n",
    "for ch in text:\n",
    "    if ch in char_count:\n",
    "        char_count[ch] += 1\n",
    "    else:\n",
    "        char_count[ch] = 1\n",
    "\n",
    "print(\"Character Frequency:\")\n",
    "for ch, count in char_count.items():\n",
    "    print(f\"{ch}: {count}\")"
   ]
  }
