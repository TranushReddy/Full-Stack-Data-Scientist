   "source": [
    "import os\n",
    "\n",
    "txt_count = 0\n",
    "py_count = 0\n",
    "\n",
    "for filename in os.listdir(\".\"):\n",
    "    if filename.endswith(\".txt\"):\n",
    "        txt_count += 1\n",
    "    elif filename.endswith(\".py\"):\n",
    "        py_count += 1\n",
    "\n",
    "print(f\"Text files: {txt_count}\")\n",
    "print(f\"Python files: {py_count}\")"
   ]
  }
