   "source": [
    "def factorial(n):\n",
    "    if n == 0 or n == 1:\n",
    "        return 1\n",
    "    return n * factorial(n - 1)\n",
    "\n",
    "\n",
    "n = int(input(\"Enter a number to compute its factorial: \"))\n",
    "print(factorial(n))"
   ]
  }
