    "import random\n",
    "\n",
    "\n",
    "def weather_simulator():\n",
    "    weather_options = [\"Sunny\", \"Cloudy\", \"Rainy\", \"Windy\", \"Stormy\"]\n",
    "    temp = random.randint(15, 40)\n",
    "    weather = random.choice(weather_options)\n",
    "    print(f\"Weather: {weather}, Temp: {temp}°C\")\n",
    "\n",
    "\n",
    "weather_simulator()"
   ]
