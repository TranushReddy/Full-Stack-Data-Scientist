   "source": [
    "def is_palindrome(s):\n",
    "    cleaned = \"\".join(s.lower().split())\n",
    "    return cleaned == cleaned[::-1]\n",
    "\n",
    "\n",
    "s = input(\"Enter a string: \")\n",
    "print(is_palindrome(s))"
   ]
  }
