   "source": [
    "marks = list(map(int, input(\"Enter marks separated by spaces: \").split()))\n",
    "\n",
    "highest = max(marks)\n",
    "lowest = min(marks)\n",
    "average = sum(marks) / len(marks)\n",
    "\n",
    "print(f\"Highest Marks = {highest}\")\n",
    "print(f\"Lowest Marks = {lowest}\")\n",
    "print(f\"Average Marks = {average:.1f}\")"
   ]
  }
