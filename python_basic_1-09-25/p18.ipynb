   "source": [
    "marks = [int(input(f\"Enter mark {i+1}: \")) for i in range(5)]\n",
    "percent = sum(marks) / 5\n",
    "\n",
    "if percent >= 90:\n",
    "    grade = \"A\"\n",
    "elif percent >= 75:\n",
    "    grade = \"B\"\n",
    "elif percent >= 50:\n",
    "    grade = \"C\"\n",
    "else:\n",
    "    grade = \"Fail\"\n",
    "\n",
    "print(\"Percentage:\", percent, \"Grade:\", grade)\n"
   ]
  }
