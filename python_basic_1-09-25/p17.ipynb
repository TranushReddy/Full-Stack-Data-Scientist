   "source": [
    "num = int(input(\"Enter a number: \"))\n",
    "div_sum = sum(i for i in range(1, num) if num % i == 0)\n",
    "\n",
    "if div_sum == num:\n",
    "    print(\"Perfect Number\")\n",
    "else:\n",
    "    print(\"Not a Perfect Number\")\n"
   ]
