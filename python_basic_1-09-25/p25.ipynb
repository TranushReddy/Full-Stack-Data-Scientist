    "months = int(input(\"Enter membership period (months): \"))\n",
    "\n",
    "cost1 = months * 500\n",
    "cost2 = (months // 6) * 2700 + (months % 6) * 500\n",
    "cost3 = (months // 12) * 5000 + (months % 12) * 500\n",
    "\n",
    "print(\"Cheapest option:\", min(cost1, cost2, cost3))\n"
