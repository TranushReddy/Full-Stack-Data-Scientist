   ],
   "source": [
    "a = int(input())\n",
    "leap = False\n",
    "if (a % 4 == 0 and (a % 100 != 0 or a % 400 == 0)):\n",
    "    leap = True\n",
    "\n",
    "if leap:\n",
    "    print(a, 'is a leap year')\n",
    "else:\n",
    "    print(a, 'is not a leap year')\n",
    "\n"
   ]
  }
